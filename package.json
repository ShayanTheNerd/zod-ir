{
  "name": "zod-ir",
  "version": "1.4.0",
  "description": "Essential Zod validators for Iranian specific data (National Code, Card Number, Mobile)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "zod",
    "validation",
    "typescript",
    "iran",
    "iranian",
    "melli-code",
    "national-code",
    "shenase-melli",
    "sheba",
    "iban",
    "bank-card",
    "mobile",
    "phone-number",
    "passport",
    "farsi-digits",
    "schema-validation",
    "react-hook-form",
    "nextjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reza-kh80/zod-ir.git"
  },
  "bugs": {
    "url": "https://github.com/Reza-kh80/zod-ir/issues"
  },
  "homepage": "https://github.com/Reza-kh80/zod-ir#readme",
  "author": "Reza kheradmandi <r.kheradmandi70@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "zod": ">=3"
  }
}
